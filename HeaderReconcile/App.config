<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings file="C:\Logility\RO\MIDSettings.config">
    <!-- ================================== -->
    <!-- ================================== -->
    <!-- HEADER RECONCILE STARTUP SETTINGS  -->
    <!-- ================================== -->
    <!-- ================================== -->

    <!-- Common settings can be found in MIDSettings.config.  To override the value in MIDSettings.config -->
    <!-- simply include the key with the new value in this config file. -->

    <!-- KEY                            DESCRIPTION -->
    <!-- ===                            =========== -->
    <!-- InputDirectory                 Required - Identifies the default input directory.  This directory will be used if not specified on the command line. -->
    <!-- OutputDirectory                Required - Identifies the default output directory.  This directory will be used if not specified on the command line. -->
    <!-- TriggerSuffix                  Required - Identifies the suffix of the input and output trigger file.  -->
    <!-- RemoveTransactionsFileName     Required - Identifies the file name for the remove transactions generated by the header reconcile process. -->
    <!-- RemoveTransactionsTriggerSuffix    Required - Identifies the suffix of the remove transactions trigger file.  -->
    <!-- HeaderTypes                    Required - "All" or Any combination of: Receipt, PO, ASN, Dummy, Dropship, Reserve, WorkupTotalBuy, VSW delimited by a comma. -->

    <add key="InputDirectory" value="C:\Logility\ROData\Header Reconcile"/>
    <add key="OutputDirectory" value="C:\Logility\ROData\Headers"/>
    <add key="TriggerSuffix" value="trg"/>
    <add key="RemoveTransactionsFileName" value="HeaderReconcileRemoveTransactions"/>
    <add key="RemoveTransactionsTriggerSuffix" value="trg"/>
    <add key="HeaderTypes" value="All"/>

  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1"/>
  </startup>
</configuration>
