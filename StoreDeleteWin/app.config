<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler"/>
  </configSections>
  <appSettings file="C:\MIDRetail\MIDSettings.config">
    <!-- ========================================= -->
    <!-- ========================================= -->
    <!-- STORE DELETE STARTUP SETTINGS -->
    <!-- ========================================= -->
    <!-- ========================================= -->
    <!-- Common settings can be found in MIDSettings.config.  To override the value in MIDSettings.config -->
    <!-- simply include the key with the new value in this config file. -->
    <!-- ====================== -->
    <!-- Services Configuration -->
    <!-- ====================== -->
    <!-- KEY						DESCRIPTION -->
    <!-- ===                        =========== -->
    <!-- DatabaseCommandTimeOut		Optional - The time (in seconds) to wait for the command to execute. The default is 30 seconds. -->
    <!-- DatabaseRetryCount			Optional - The number of times to retry a database call before failing the call. The default is 4 times.-->
    <!--							This can be set to 0 if you do not want the application to attempt to retry the call. -->
    <!-- CopyConcurrentProcesses		Optional - This is a tuning option.  It identifies the number of concurrent Copy/Load tables -->
    <!--							that are process in tandem.  The default is 1. -->
    <!-- DeleteConcurrentProcesses		Optional - This is a tuning option.  It identifies the number of concurrent Delete from tables -->
    <!--							that are process in tandem.  The default is 1. -->
    <!-- BatchSize				Optional - This is a tuning option.  It identifies the number within each batch during a delete process. -->
    <!--						The default is 2000. -->
    <!-- AnalysisOnly		Optional - performs data analysis without removing any data.  The default is TRUE. -->
    <!-- SkipAnalysis		Optional - Skips the table analysis step. ASSUMES the table analysis has been run manually.  The default is FALSE. -->
    <!-- MinimumRowCount		Optional - If rows to delete are <= this value, results will be to delete table.  The default is 20. -->
    <!-- MaximumRowCount		Optional - If the rows to be removed are >= than this value, results will be to force copying the table..  The default is 500,000. -->
    <!-- RowPercentageThreshold		Optional - If percentage of rows to delete are <= this value, results will be to delete table..  The default is 50000. -->

    <!-- //BEGIN TT#4636-VStuart-Incorrectly checking if services are down-MID -->
    <!--<add key="ControlServiceNames" value="MIDRetailControlService.exe;"/>
    <add key="StoreServiceNames" value="MIDRetailStoreService.exe;"/>
    <add key="MerchandiseServiceNames" value="MIDRetailHierarchyService.exe;"/>
    <add key="SchedulerServiceNames" value="MIDRetailSchedulerService.exe;"/>
    <add key="ApplicationServiceNames" value="MIDRetailApplicationService.exe;"/>-->
    <add key="ControlServiceNames" value="MIDRetailControlService"/>
    <add key="StoreServiceNames" value="MIDRetailStoreService"/>
    <add key="MerchandiseServiceNames" value="MIDRetailHierarchyService"/>
    <add key="SchedulerServiceNames" value="MIDRetailSchedulerService"/>
    <add key="ApplicationServiceNames" value=""/>
    <!-- //END TT#4636-VStuart-Incorrectly checking if services are down-MID -->

    <add key="CopyConcurrentProcesses" value="4"/>
    <add key="DeleteConcurrentProcesses" value="1"/>
    <add key="DatabaseCommandTimeOut" value="0"/>
    <add key="BatchSize" value="2500"/>
    <add key="SkipAnalysis" value="false"/>
    <add key="AnalysisOnly" value="true"/>
    <add key="MinimumRowCount" value="5000"/>
    <add key="MaximumRowCount" value="25000"/>
    <add key="RowPercentageThreshold" value="15"/>

  </appSettings>
  <log4net>
    <logger name="Activity">
      <!--<level value="ALL" />-->
      <!--<level value="DEBUG" />-->
      <level value="INFO"/>
      <!--<level value="WARN" /> -->
      <!--<level value="ERROR" /> -->
      <!--<level value="FATAL" /> -->
      <!--<level value="OFF" /> -->
      <appender-ref ref="ActivityLogFileAppender"/>
    </logger>
    <!-- Define some output appenders -->
    <appender name="ActivityLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="c:\MIDRetail\StoreDeleteActivity.log"/>
      <appendToFile value="false"/>
      <maxSizeRollBackups value="30"/>
      <maximumFileSize value="5000KB"/>
      <rollingStyle value="Size"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date{yyyy-MM-dd HH:mm:ss.fff} %-5level %message%newline"/>
      </layout>
    </appender>
  </log4net>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/></startup></configuration>
