  <!-- Load text procedures and any other functions/views/procedures used when loading calcs and variables -->

  <!-- Load the calcs and variables -->
  <SQLObject Process="LoadCalcsAndVariables"></SQLObject>
  
  <!-- When upgrading the database, upgrade the variable tables right after you would have created them during a build new database -->
  <SQLObject Process="UpdateVariableTables"></SQLObject>

  <!-- Always regenerate stored procedures, functions, views, etc.  After the tables have been created or updated. -->
  <SQLObject Process="GenerateNonTableFiles"></SQLObject>
