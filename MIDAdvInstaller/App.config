<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    
    <!--application file names -->
    <add key="ClientNames" value="MIDRetail.exe;"/>
    <add key="ControlServiceNames" value="MIDRetailControlService.exe;"/>
    <add key="StoreServiceNames" value="MIDRetailStoreService.exe;"/>
    <add key="MerchandiseServiceNames" value="MIDRetailHierarchyService.exe;"/>
    <add key="JobServiceNames" value="MIDRetailJobService.exe;" />
    <add key="ROWebServiceNames" value="ROWebHost.exe;" />
