    <add key="SupportedWindowsServerVersions" value="WindowsXP;WindowsServer2008;Windows7;WindowsServer2008R2;Windows8;WindowsServer2012;WindowsServer2016"/>

    <!--configuration file names -->
    <add key="MIDSettings_config" value="MIDSettings.config"/>
    <add key="MIDRetail_config" value="MIDRetail.exe.config"/>

    <!--defaults -->
    <add key="InstallClientFolder" value="Client"/>
