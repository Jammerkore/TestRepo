    <add key="InstallMerchandiseServiceFolder" value="Hierarchy Service"/>
    <add key="InstallJobServiceFolder" value="Job Service" />
    <add key="InstallROWebServiceFolder" value="RO Web Service" />
    <add key="InstallStoreServiceFolder" value="Store Service"/>
    <add key="InstallSchedulerServiceFolder" value="Scheduler Service"/>
    <add key="InstallAPIFolder" value="Batch"/>
    <add key="ClientInstallLocation" value="C:\Logility\RO"/>
    <add key="ServicesInstallLocation" value="C:\Logility\RO"/>
    <add key="ServicesConfigurationLocation" value="C:\Logility\RO\"/>
